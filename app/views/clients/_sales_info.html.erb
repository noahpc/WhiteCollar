<table class="display default_table">
  <thead>
  <tr>
    <th>Year</th>
    <th>Type</th>
    <th>Sale Value</th>
    <th>Page Size</th>
    <th>Payment Type</th>
  </tr>
  </thead>
    <tbody>
    	  <% sales_info.each do |s| %>
      <tr>
        <td align="right"><%= s.ticket.project.year %></td>
        <td align="right"><%= s.ticket.project.project_type.name  %></td>
        <td align="right"><%= s.sale_value %></td>
        <td align="right"><%= s.page_size %></td>
        <td align="right"><%= s.payment_type %></td>
      </tr>
  <% end %>
  </tbody>
</table>