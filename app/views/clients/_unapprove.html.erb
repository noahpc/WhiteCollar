
<br /><br />
<table class="display">
  <thead>
  <tr>
    <th>Business name</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Telephone</th>
    <th>Contact title</th>
    <th>First name</th>
    <th>Last name</th>
    <th>E-mail</th>
    <th>Comment</th>
  </tr>

  </thead>

  <tbody>
  <% unapproved_clients.each do |client|%>
      <tr>
      	<% if current_user.role == 3 %>
        	<td><%= link_to client.business_name, client_path(client.id, from: 'teacher' ) %></td>
        <% else %>
        	<td><%= link_to client.business_name, client_path(client.id, from: 'student' ) %></td>
		<% end %>
        <td><%= client.address %></td>
        <td><%= client.city %></td>
        <td><%= client.state %></td>
        <td><%= client.zipcode %></td>
        <td><%= client.telephone %></td>
        <td><%= client.contact_title %></td>
        <td><%= client.contact_fname %></td>
        <td><%= client.contact_lname %></td>
        <td><%= client.email %></td>
        <td><%= client.comment %></td>
      </tr>
  <% end %>
  </tbody>
</table>
