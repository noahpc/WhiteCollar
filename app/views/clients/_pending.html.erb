<h1>Pending Clients</h1>

<table class="table">
  <thead>
    <tr>
      <th>Business name</th>
      <th>Address</th>
      <th>Email</th>
      <th>Title</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Telephone</th>
      <th>Comment</th>
    </tr>
    
  </thead>

<tbody>
	<% pending_clients.each do |client|%>
	<tr>
		<td><%= client.business_name %></td>
        <td><%= client.address %></td>
        <td><%= client.email %></td>
        <td><%= client.contact_title %></td>
        <td><%= client.contact_fname %></td>
        <td><%= client.contact_lname %></td>
        <td><%= client.telephone %></td>
        <td><%= client.comment %></td>
      </tr>
	</tr>
	<% end %>
	</tbody>
	</table>
