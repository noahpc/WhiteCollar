	<!-- Here is huge table of all the students -->
	<table class ="table dataTable">
	  <thead>
	    <tr>
	      <th></th>
	      <th>First Name</th>
	      <th>Last Name</th>
	      <th>ID number</th>
	      <th>Email</th>
	      <th>Phone</th>
	      <th>Student Manager</th>
	      <th>Section</th>
		  <th>Major</th>
		  <th>Minor</th>
		  <th>Box</th>
		  <th>Classification</th>
		  <th></th>      	      	      	      	      
	    </tr>
	  </thead>
	  <tbody>
            <% current_students.each do |student| %>
              <tr>
                <td><%= check_box_tag "students[]", student.id%></td>
                <td><%= link_to student.first_name, "/receipts/my_receipts/#{student.id}" %></td>
                <td><%= link_to student.last_name, "/receipts/my_receipts/#{student.id}" %></td>
                <td><%= student.school_id %></td>
                <td><%= student.email %></td>
                <td><%= student.phone %></td>                
                <td><%= student.student_manager_name %></td>
                <td><%= student.section_number %></td>
                <td><%= student.major %></td>
                <td><%= student.minor %></td>
                <td><%= student.box %></td>
                <td><%= student.class %></td>       
                <td><%= link_to 'Edit', edit_user_path(student.id) %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
