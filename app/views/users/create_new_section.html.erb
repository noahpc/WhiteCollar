<%= form_tag('/users/assign_teacher_to_section') do %>

<!-- Gets rid of the section number if it has already been taken -->
<% select = Array[]%>
<% @good_selection.each do |selection| %>
	<% select.push(selection) %>
<% end %>

<%=label_tag "Please select the section number:" %>
<%=select_tag :section, options_for_select(select) %>

<br><br>
<%=label_tag "Please select the teacher:" %>
<%= collection_select  :last_name, :id, User.all_teachers, :id, :last_name  %>

<br><br>

<%= submit_tag "Create this section" %>

<% end %>