<%= debug @all_receipts%>

<h1>My Current Clients</h1>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Contact First Name</th>
      <th>Contact Last Name</th>
      <th>Business Name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Telephone</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @active_tickets.each do |ticket| %>
    <% @client = Client.find(ticket.id)%>
      <tr>
      	<td><%= @client.contact_title%></td>
      	<td><%= @client.contact_fname%></td>
      	<td><%= @client.contact_lname%></td>
      	<td><%= @client.business_name%></td>
      	<td><%= @client.address%></td>
      	<td><%= @client.city%></td>
      	<td><%= @client.state%></td>
      	<td><%= @client.zipcode%></td>
      	<td><%= @client.telephone%></td>
      	<td><%= @client.email%></td>
        <td><%= link_to 'Show', ticket %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<br/>

