#######################################################################################################################################

<% @active_tickets.each do |ticket| %>
    <% client = Client.find(ticket.client_id)%>
      <div class="active <%= Priority.find(ticket.priority_id).name%>">
      	 <span><%= client.contact_title%></span> 
      	 <span><%= client.contact_fname%></span>
      	 <span><%= client.contact_lname%></span> 
      	 <span><%= client.business_name%></span> 
      	 <span><%= client.address%></span> 
      	 <span><%= client.city%></span> 
      	 <span><%= client.state%></span> 
      	 <span><%= client.zipcode%></span> 
      	 <span><%= client.telephone%></span> 
      	 <span><%= client.email%></span> 
         <% receipt = Receipt.where("ticket_id = ? AND user_id = ?",ticket.id, @student_user.id).first %>
         <span><%= link_to 'Show', receipt_path(receipt) %></span> 
      </div>
      <br /><br />
<% end %>

