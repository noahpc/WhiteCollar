<table class ="table dataTable">
<thead>
<tr>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Student Manager</th>
  <th>Section</th>
  <th>Date/Time activity was reported</th>
  <th>Client</th>
  <th>Activity</th>
  <th>Points Received</th>
  <th>Cumulative Points on Client</th>
  <th>Comments</th>
</tr>
</thead>
<tbody>
<tbody>
    <% @activities.each do |activity| %>
      <tr>
        <td><%= link_to activity.first_name, "/receipts/my_receipts/#{activity.student_id}"%></td>
        <td><%= link_to activity.last_name, "/receipts/my_receipts/#{activity.student_id}"%></td>
        <td><%= activity.team_leader%></td>
        <td><%= activity.section%></td>
        <td><%= activity.time_of_activity%></td>
        <td><%= activity.company%></td>
        <td><%= activity.activity%></td>
        <td><%= activity.points_earned%></td>
        <td><%= activity.cumulative_points_earned_on_client%></td>
        <td><%= activity.comments%></td>        
      </tr>
    <% end %>
</tbody>
</table>